pipeline {
  agent any;
  stages {
    stage ('BUILD') {
      steps {
        echo "This is build"
          sh 'sleep 5'
      }
    }
     stage ('DEPLOY') {
      steps {
        echo "This is deploy"
         sh 'sleep 5'
      }
    }
     stage ('TEST') {
      steps {
        echo "This is test"
         sh 'sleep 5'
      }
    }
  }
}
